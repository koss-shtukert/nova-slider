{
    "name": "rp/nova-slider",
    "description": "A Laravel Nova filter",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "RP\\Filters\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RP\\Filters\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
